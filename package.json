{
  "name": "form-builder",
  "version": "1.0.0",
  "description": "",
  "main": "build/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "db": "npx knex migrate:latest --knexfile build/src/database/knexfile.js",
    "migrate": "npx knex migrate:latest --knexfile src/database/knexfile.js",
    "migrate-down": "npx knex migrate:down --knexfile src/database/knexfile.js",
    "create-migration": "knex migrate:make -x ts --migrations-directory src/database/migrations",
    "start": "ts-node build/src/index.js",
    "build": "tsc",
    "dev": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "knex": "^0.95.15",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.8",
    "@types/validator": "^13.7.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
